<!DOCTYPE>
<html ng-app="aksc">	
<head>
<title>Decrypt Leaderboard</title>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
 <!-- Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css">
			<link rel="icon" type="/images/png" href="assets/images/fav.png"/>

  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>
<style type="text/css">
.nav-wrapper {
  background-color: #ff9900 !important;
}
</style>
<title>AKSC Admin</title>
</head>
<body ng-controller="adminController">
	<nav>
	    <div class="nav-wrapper">
	      <a href="#" class="brand-logo" style="padding-left:10px">Decrypt Leaderboard - Round 1</a>
	    </div>
	  </nav>
	<div class="container col-md-12" style="height:100%;width:150%">
	<table class="table">
		<thead>
			<tr>
				
				<th>ID</th>
				<th>Name</th>
				<th>College</th>
				<th>Rank</th>
				
		</thead>
		<tbody>
					<% for(var i=0;i<result.length;i++){ %>

			<tr>
				<td><b><%= result[i].ieee %></b></td>
				<td><b><%= result[i].name %></b></td>
				<td><b><%= result[i].college %></b></td>
				<td><%= i+1 %></td>
				
			</tr>
			<% } %>
		</tbody>
	</table>
    </div>
<!-- <input ng-model="test"><p>{{test}}</p> -->
		<script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.6.5/angular.min.js"></script>
    <script src="./bower_components/jquery/dist/jquery.min.js"></script>
    <script src="./assets/js/ResponsiveSlides.js/responsiveslides.min.js"></script>
    <script src="./bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="./bower_components/smooth-scroll/dist/js/smooth-scroll.min.js"></script>
    <script src="./assets/js/main.js"></script>
    <script src="./assets/js/controllers.js"></script>
</body>
</html>
